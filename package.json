{
  "name": "rclone-drive-webui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare-react-pdf": "cp ./node_modules/pdfjs-dist/build/pdf.worker.js ./public/pdf.worker.js",
    "clean": "rm -rf ./node_modules && rm -rf ./build",
    "prepare": "husky install",
    "start-dev": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint 'src/**/*.js' && stylelint 'src/**/*.scss' && prettier --check .",
    "lint:fix": "eslint --fix 'src/**/*.js' && stylelint --fix 'src/**/*.scss' && prettier --write .",
    "test": "react-scripts test --watchAll=false --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.4",
    "@mui/lab": "^5.0.0-alpha.53",
    "@mui/material": "^5.0.4",
    "axios": "^0.23.0",
    "classnames": "^2.3.1",
    "file-saver": "^2.0.5",
    "lru-cache": "^6.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-file-viewer": "^1.2.1",
    "react-intersection-observer": "^8.32.2",
    "react-pdf": "^5.5.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.6",
    "sass": "^1.43.3",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "web-vitals": "^1.0.1",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@testing-library/dom": "^8.11.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^13.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^react-pdf/dist/umd/entry.webpack$": "react-pdf"
    }
  }
}
